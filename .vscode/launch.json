    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch)",
            "request": "launch",
            "mainClass": "com.google.javascript.jscomp.CommandLineRunner",
            "classPaths": ["./target/closure-compiler-1.0-SNAPSHOT.jar"],
            "args": " --compilation_level ADVANCED test.js",
            "sourcePaths": ["${workspaceFolder}/src"],
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        }
    ]
}